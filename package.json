{
  "name": "@fixers/stripe-js",
  "type": "module",
  "version": "0.0.0",
  "description": "Naked loading wrapper for Stripe.js ",
  "author": "Luca <git@lucacicada.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wefixers/stripe-js.git"
  },
  "keywords": [
    "stripe",
    "stripe-js"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
