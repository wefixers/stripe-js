  "dependencies": {
    "@stripe/stripe-js": "^1.54.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "changelogen": "^0.5.3",
    "eslint": "^8.42.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  }
}
